<?xml version="1.0" encoding="UTF-8"?>
<list>
	<name>Připojení</name>
	<author>GnuSkola</author>
	<apps>
		<app>
			<name>Aplikace pro připojení Putty</name>
			<comm>apt-get install --allow-unauthenticated putty -y --force-yes</comm>
			<uni>apt-get autoremove putty -y --force-yes</uni>
		</app>
		<app>
			<name>Aplikace pro připojení Minicom</name>
			<comm>apt-get install --allow-unauthenticated minicom -y --force-yes</comm>
			<uni>apt-get autoremove minicom -y --force-yes</uni>
		</app>
	</apps>
	<note>Seznam aplikací vhodných pro připojení k dalším zařízením</note>
</list>