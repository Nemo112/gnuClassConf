<?xml version="1.0" encoding="UTF-8"?>
<list>
	<name>Grafické editory</name>
	<author>GnuSkola</author>
	<apps>
		<app>
			<name>Prohlížeč obrázků Gpicview</name>
			<comm>apt-get install --allow-unauthenticated gpicview -y --force-yes</comm>
			<uni>apt-get autoremove gpicview -y --force-yes</uni>
		</app>
		<app>
			<name>Editor obrázků Gimp</name>
			<comm>apt-get install --allow-unauthenticated gimp -y --force-yes</comm>
			<uni>apt-get autoremove gimp -y --force-yes</uni>
		</app>
		<app>
			<name>Editor obrázků InkScape</name>
			<comm>apt-get install --allow-unauthenticated inkscape -y --force-yes</comm>
			<uni>apt-get autoremove inkscape -y --force-yes</uni>
		</app>
	</apps>
	<note>Seznam aplikací vhodných pro kancelářskou práci</note>
</list>