<?xml version="1.0" encoding="UTF-8"?>
<list>
	<name>Psaní deseti</name>
	<author>GnuSkola</author>
	<apps>
		<app>
			<name>Psaní s Klavaro</name>
			<comm>apt-get install --allow-unauthenticated klavaro -y --force-yes</comm>
			<uni>apt-get autoremove klavaro -y --force-yes</uni>
		</app>
		<app>
			<name>Psaní s Ktouch</name>
			<comm>apt-get install --allow-unauthenticated ktouch -y --force-yes</comm>
			<uni>apt-get autoremove ktouch -y --force-yes</uni>
		</app>
		<app>
			<name>Hra Tux typing</name>
			<comm>apt-get install --allow-unauthenticated tuxtype -y --force-yes</comm>
			<uni>apt-get autoremove tuxtype -y --force-yes</uni>
		</app>
	</apps>
	<note>Seznam aplikací vhodných pro psaní všemi deseti</note>
</list>